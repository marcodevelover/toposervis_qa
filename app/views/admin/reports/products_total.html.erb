<%= search_form_for @q, url: admin_reports_products_path do |f| %> 
            <!-- START card-->
            <div class="card card-default">
               <div class="card-body">
                  
                     <h3></h3>
                     <fieldset>
                        <div class="form-group row">
                           <div class="col-md-12">
                              <div class="form-row">
                                <div class="col-lg-6 mb-6">
                                  <label class="form-label">Fecha Inicio</label>
                                  <div class="form-group"> 
                                    <%= f.search_field :created_at_gteq, class: "form-control" , placeholder: "Buscar", type: 'date'%>
                                  </div>  
                                </div> 
                                <div class="col-lg-6 mb-6">
                                  <label class="form-label">Fecha Final</label>
                                  <div class="form-group"> 
                                    <%= f.search_field :created_at_lteq, class: "form-control" , placeholder: "Buscar", type: 'date'%>
                                  </div>  
                                </div>
                              </div>
                          </div>
                        </div>

                        <div class="form-group row">
                           <div class="col-md-12">
                              <div class="form-row">
                                <div class="col-lg-6 mb-6">
                                  <label class="form-label">Nombre</label>
                                  <div class="form-group"> 
                                    <%= f.search_field :name_cont, class: "form-control" , placeholder: "Buscar"%>
                                  </div>  
                                </div> 
                                <div class="col-lg-6 mb-6">
                                  <label class="form-label">Marca</label>
                                  <div class="form-group"> 
                                    <%= f.search_field :brand_cont, class: "form-control" , placeholder: "Buscar"%>
                                  </div>  
                                </div>                                 
                              </div>
                          </div>
                        </div>
                        <div class="form-group row">
                           <div class="col-md-12">
                              <div class="form-row">
                                <div class="col-lg-12 mb-12">
                                  <label class="form-label">Modelo</label>
                                  <div class="form-group"> 
                                    <%= f.search_field :model_cont, class: "form-control" , placeholder: "Buscar"%>
                                  </div>  
                                </div>                                 
                              </div>
                          </div>
                        </div><br>
                        <a href="<% admin_reports_products_path %>?per_page=100000">Mostrar todo</a>
	                     <div class="clearfix">
	                        <div class="float-right">
	                        <%= f.submit class: "btn btn-primary" %>

	                        
	                        </div>
	                     </div>
<% end %>
<% if @collection %><%= render "products_total" %><%end%>
                      </fieldset>
               </div> <!--card-body card-->
            </div>
            <!-- END card-->

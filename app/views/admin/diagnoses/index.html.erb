<p id="notice"><%= notice %></p>

<h1>Diagnoses</h1>

<table>
  <thead>
    <tr>
      <th>Service order</th>
      <th>Date</th>
      <th>Delivery time</th>
      <th>Date delivery</th>
      <th>Diagnosis type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diagnoses.each do |diagnosis| %>
      <tr>
        <td><%= diagnosis.service_order_id %></td>
        <td><%= diagnosis.date %></td>
        <td><%= diagnosis.delivery_time %></td>
        <td><%= diagnosis.date_delivery %></td>
        <td><%= diagnosis.diagnosis_type_id %></td>
        <td><%= link_to 'Show', diagnosis %></td>
        <td><%= link_to 'Edit', edit_diagnosis_path(diagnosis) %></td>
        <td><%= link_to 'Destroy', diagnosis, method: :delete, data: { confirm: 'Â¿Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


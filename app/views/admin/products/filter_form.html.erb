  	 <%= search_form_for [:admin, @q], remote:true do |f| %> 
			<div class="modal-body">
				<div class="form-group">
					<div class="form-group"> 
						<label class="form-label">Nombre</label>
						<div class="form-group">
							<%= search_field_tag  "q[name_cont]",nil,class: "form-control", placeholder: "Nombre", width: '100%'%>
						</div>
					</div>
					<div class="form-group"> 
						<label class="form-label">Marca</label>
						<div class="form-group">
							<%= search_field_tag "q[brand_cont]",nil,class: "form-control" , placeholder: "Marca" %>
						</div>
					</div>
					<div class="form-group"> 
						<label class="form-label">Modelo</label>
						<div class="form-group">
							<%= search_field_tag "q[model_cont]",nil,class: "form-control" , placeholder: "Modelo" %>
						</div>  
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<%= submit_tag t('search'),class: 'btn btn-primary waves-effect' %> 
				<button type="button" class="btn btn-danger waves-effect" data-dismiss="modal">Cancelar</button>
			</div>
		<% end %>